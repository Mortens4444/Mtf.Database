<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Mtf.Database</id>
    <version>2.0.41</version>
    <description>
      Database connections (SqlClient, SQLite).

      Compatibility:
      - .NET Framework 4.6.2
      - .NetStandard 2.0
	</description>
    <authors>Mortens</authors>
	<owners>Mortens</owners>
    <copyright>Copyright © 2024</copyright>
	<license type="expression">MIT</license>
	<projectUrl>https://github.com/Mortens4444/Mtf.Database</projectUrl>
    <repository type="git" url="https://github.com/Mortens4444/Mtf.Database.git" branch="main" />
	<tags>SqlClient Sqlite Database BaseRepository Mortens</tags>
	<readme>docs\ReadMe.md</readme>
    <dependencies>
      <group targetFramework="net452">
        <dependency id="Dapper" version="1.60.6" />
	  </group>
      <group targetFramework="net462">
        <dependency id="Dapper" version="2.1.66" />
        <dependency id="Microsoft.Data.SqlClient" version="6.0.2" />
        <dependency id="Microsoft.Data.SQLite" version="9.0.5" />
        <dependency id="Microsoft.Data.SQLite.Core" version="9.0.5" />
	  </group>
      <group targetFramework="netstandard2.0">
        <dependency id="Dapper" version="2.1.66" />
        <dependency id="Microsoft.Data.SqlClient" version="6.0.2" />
        <dependency id="Microsoft.Data.SQLite" version="9.0.5" />
        <dependency id="Microsoft.Data.SQLite.Core" version="9.0.5" />
	  </group>
    </dependencies>
  </metadata>
  <files>
    <file src=".\Mtf.Database\bin\Release\net452\**" target="lib\net452" />
    <file src=".\Mtf.Database\bin\Release\net462\**" target="lib\net462" />
    <file src=".\Mtf.Database\bin\Release\netstandard2.0\**" target="lib\netstandard2.0" />
	<file src=".\ReadMe.md" target="docs\" />
  </files>
</package>
